<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <Rules>
        <rules index="1" regex="(.*)" description="Everything including &gt;" />
        <rules index="2" regex="&gt;(.*)" description="Everything after &gt;" />
        <rules index="3" regex="&gt;([^ ]*)" description="Up to first space" />
        <rules index="4" regex="&gt;IPI:([^\| .]*)" description="IPI accession" />
        <rules index="5" regex="&gt;(gi\|[0-9]*)" description="NCBI accession" />
        <rules index="6" regex="&gt;([^\t]*)" description="Up to first tab character" />
        <rules index="7" regex="&gt;.*\|(.*)\|" description="Uniprot" />
        <rules index="8" regex="&gt;([^\|]*)" description="up to first \ or |" />
